{
	"cSpell.enabled": true,
	"docwriter.style": "RustDoc",
	"rust-analyzer.cargo.features": [
		"tauri"
	],
	"conventionalCommits.scopes": [
		"deps"
	],
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.check.extraArgs": [
		"--",
		"-W",
		"clippy::pedantic",
		"-W",
		"clippy::cargo",
		"-W",
		"clippy::indexing_slicing",
		"-A",
		"clippy::module_name_repetitions",
	]
}
