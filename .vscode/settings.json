{
  "cSpell.enabled": true,
  "docwriter.style": "RustDoc",
  "cSpell.words": [
    "Butcherable",
    "Carn",
    "dfraw",
    "gamelog",
    "GIANTAXEBLADE",
    "Hiveable",
    "itertools",
    "Megabeast",
    "MINIFORGE",
    "powf",
    "SIEGEAMMO",
    "SMALLGEMS",
    "TRAPCOMP",
    "typetag",
    "VAMPCURSE",
    "walkdir"
  ],
  "rust-analyzer.cargo.features": ["tauri"],
  "conventionalCommits.scopes": ["deps"],
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.check.extraArgs": ["--", "-W", "clippy::pedantic", "-W", "clippy::cargo", "-W", "clippy::indexing_slicing"]
}
