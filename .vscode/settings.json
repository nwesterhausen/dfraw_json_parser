{
  "cSpell.enabled": true,
  "docwriter.style": "RustDoc",
  "cSpell.words": ["dfraw", "gamelog", "Megabeast", "powf", "rouille", "walkdir"],
  "rust-analyzer.cargo.features": ["tauri"],
  "rust-analyzer.checkOnSave.features": "all",
  "rust-analyzer.checkOnSave.command": "clippy",
  "rust-analyzer.checkOnSave.extraArgs": ["--", "-W", "clippy::cargo"],
  "conventionalCommits.scopes": ["deps"]
}
